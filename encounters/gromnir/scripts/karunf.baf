IF
	See([PC])
	Allegiance(Myself,ENEMY)
	Global("SPRITE_IS_DEADgromg08","GLOBAL",0)
	Global("VP_Karun_Multiplies","GLOBAL",0)
THEN
	RESPONSE #100
		SetGlobal("VP_Karun_Multiplies","GLOBAL",1)
		CreateCreatureObjectCopy("gromg08",Myself,0,0,0)  // Karun the Black
		CreateCreatureObjectCopy("gromg08",Myself,0,0,0)  // Karun the Black
		CreateCreatureObjectCopy("gromg08",Myself,0,0,0)  // Karun the Black
		CreateCreatureObjectCopy("gromg08",Myself,0,0,0)  // Karun the Black
		CreateCreatureObjectCopy("gromg08",Myself,0,0,0)  // Karun the Black
		CreateCreatureObjectCopy("gromg08",Myself,0,0,0)  // Karun the Black
END

IF
	Die()
THEN
	RESPONSE #100
		IncrementGlobal("VP_Fake_Karun_Dead","GLOBAL",1)
		DestroySelf()
END

IF
	RandomNum(9,1)
	OR(2)
		GlobalLT("VP_Fake_Karun_Dead","GLOBAL",7)
		GlobalLT("SPRITE_IS_DEADgromg08","GLOBAL",7)
THEN
	RESPONSE #100
		SetGlobal("VP_Jump","LOCALS",1)
		JumpToPoint([643.791])
		Continue()
END

